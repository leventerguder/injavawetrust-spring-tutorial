<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="configId" class="_06.spel.model.InjectSimpleConfig" />

	<bean id="personId" class="_06.spel.model.Person">
		<property name="name" value="#{configId.name}"/>
		<property name="surname" value="#{configId.surname}"/>
		<property name="gender" value="#{configId.gender}"/>
		<property name="birthYear" value="#{configId.birthYear}"/>
	</bean>

</beans>
